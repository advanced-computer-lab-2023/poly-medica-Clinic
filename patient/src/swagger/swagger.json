                "summary": "Create a new patient",
                "description": "Creates a new patient with the provided details",
                "consumes": ["application/json"],
                "parameters": [
                  {
                    "name": "body",
                    "in": "body",
                    "description": "Patient details",
                    "required": true,
                    "schema": {
                      "$ref": "#/definitions/Patient"
                    }
