    "definitions": {
        "Patient": {
                "type": "object",
                "properties": {
                  "name": { "type": "string" },
                  "userName": { "type": "string" },
                  "email": { "type": "string" },
                  "password": { "type": "string" },
                  "dateOfBirth": { "type": "string", "format": "date" },
                  "gender": { "type": "string", "enum": ["MALE", "FEMALE"] },
                  "mobileNumber": { "type": "string" },
                  "emergencyContact": { "$ref": "#/definitions/EmergencyContact" },
                  "familyMembers": { "type": "array", "items": { "$ref": "#/definitions/FamilyMember" } },
                  "healthRecords": { "type": "array", "items": { "$ref": "#/definitions/HealthRecord" } },
                  "healthPackages": { "type": "array", "items": { "$ref": "#/definitions/HealthPackage" } },
                  "walletAmount": { "type": "number", "default": 0, "minimum": 0 },
                  "deliveryAddresses": { "type": "array", "items": { "$ref": "#/definitions/DeliveryAddress" } }
                },
                "required": ["name", "userName", "email", "password", "dateOfBirth", "gender", "mobileNumber"]
        },
        "EmergencyContact": {
            "type": "object",
            "properties": {
              "name": { "type": "string" },
              "mobile": { "type": "string" },
              "relation": { "type": "string", "enum": ["Parent", "Child", "Sibling", "Spouse", "Grandparent", "Grandchild", "Aunt", "Uncle", "Niece", "Nephew", "Cousin", "Other"] }
            },
            "required": ["name", "mobile", "relation"]
          },
        "FamilyMember": {
            "type": "object",
            "properties": {
              "id": { "type": "string" },
              "name": { "type": "string" },
              "nationalId": { "type": "string" },
              "age": { "type": "integer" },
              "gender": { "type": "string", "enum": ["MALE", "FEMALE"] },
              "relation": { "type": "string", "enum": ["HUSBAND", "WIFE", "CHILD"] },
              "email": { "type": "string" },
              "mobileNumber": { "type": "string" }
            },
            "required": ["name"]
          },
        "HealthRecord": {
            "type": "object",
            "properties": {
              "recordTitle": { "type": "string" },
              "documentName": { "type": "string" }
            },
            "required": ["recordTitle", "documentName"]
          },
        "HealthPackage": {
            "type": "object",
            "properties": {
              "packageId": { "type": "string" },
              "subscribtionDate": { "type": "string", "format": "date" },
              "renewalDate": { "type": "string", "format": "date" },
              "status": { "type": "string", "enum": ["CANCELLED", "SUBSCRIBED WITH RENEWAL DATE", "UNSUBSCRIBED"] }
            },
            "required": ["packageId", "subscribtionDate", "status"]
          },
        "DeliveryAddress": {
            "type": "object",
            "properties": {
              "city": { "type": "string" },
              "street": { "type": "string" },
              "buildingName": { "type": "string" },
              "phoneNumber": { "type": "string" },
              "primary": { "type": "boolean" }
            },
            "required": ["city", "street", "buildingName", "phoneNumber"]
          },
          "Medicine": {
            "type": "object",
            "properties": {
                "medicineId": {
                    "type": "string",
                    "format": "uuid", 
                    "example": "5fde07a7b2e33d54a474597e"
                },
                "name": {
                    "type": "string",
                    "example": "Medicine A"
                },
                "price": {
                    "type": "number",
                    "example": 20.5
                },
                "amount": {
                    "type": "number",
                    "example": 3
                }
            },
            "required": ["medicineId", "name", "price", "amount"]
           },
           "Prescription": {
            "type": "object",
            "properties": {
                "patientId": {
                    "type": "string",
                    "format": "uuid",
                    "example": "5fde07a7b2e33d54a474597c"
                },
                "doctorId": {
                    "type": "string",
                    "format": "uuid",
                    "example": "5fde07a7b2e33d54a474597d"
                },
                "doctorName": {
                    "type": "string",
                    "example": "Dr. John Doe"
                },
                "date": {
                    "type": "string",
                    "format": "date-time",
                    "example": "2023-01-05T10:30:00Z"
                },
                "filled": {
                    "type": "boolean",
                    "example": true
                },
                "description": {
                    "type": "string",
                    "example": "Prescription for treating a specific condition"
                },
                "medicines": {
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/Medicine"
                    }
                },
                "inCart": {
                    "type": "boolean",
                    "default": false,
                    "example": false
                },
                "purchased": {
                    "type": "boolean",
                    "default": false,
                    "example": false
                }
            },
            "required": ["patientId", "doctorId", "date", "filled", "description", "medicines"]
        },
        "UserSignupRequest": {
            "type": "object",
            "properties": {
                "email": { "type": "string" },
                "password": { "type": "string" },
                "userName": { "type": "string" }
            },
            "required": ["email", "password", "userName"]
        },
        "UserSignupResponse": {
            "type": "object",
            "properties": {
                "userId": { "type": "string" },
                "email": { "type": "string" },
                "password": { "type": "string" },
                "userName": { "type": "string" },
                "type": { "type": "string" }
            }
        },
        "UserSignupErrorResponse": {
            "type": "object",
            "properties": {
                "errCode": { "type": "number" },
                "errMessage": { "type": "string" }
            }
        },
        "ErrorResponse": {
            "type": "object",
            "properties": {
              "error": {
                "type": "object",
                "properties": {
                  "message": { "type": "string" }
                }
              }
            }
          }
    }
  }
  